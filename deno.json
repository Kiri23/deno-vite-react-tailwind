{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/",
    "deploy": "deno task build && cd dist && deployctl deploy --project=vite-react-tailwind --entrypoint=jsr:@std/http@1/file-server",
    "deploy:custom": "deno task build && cd dist && deployctl deploy --project=$PROJECT_NAME --entrypoint=jsr:@std/http@1/file-server",
    "deploy:script": "deno run --allow-net --allow-read --allow-write --allow-env deploy.ts",
    "deploy:server": "deno run --allow-net --allow-read --allow-write --allow-env deploy.ts",
    "dev:server": "deno run --allow-net --allow-read --allow-env --unstable-kv server/server.ts"
  },
  "compilerOptions": {
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.5",
    "@types/react": "npm:@types/react@^19.1.8",
    "@types/react-dom": "npm:@types/react-dom@^19.1.6",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.6.0",
    "react": "npm:react@^19.1.0",
    "react-dom": "npm:react-dom@^19.1.0",
    "vite": "npm:vite@^7.0.0",
    "tailwindcss": "npm:tailwindcss@^4.0.0",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.0.0",
    "kiriWebFramework": "./server/lib/kiriWebFramework/mod.ts"
  },
  "unstable": ["kv"]
}
